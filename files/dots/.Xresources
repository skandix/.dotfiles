! URXVT
urxvt*font: xft:Monospace:pixelsize=12
urxvt*loginShell:         true
urxvt*scrollBar:         false
urxvt*saveLines:         65535
urxvt*cursorBlink:       false
urxvt*urgentOnBell:       true
urxvt*scrollTtyKeypress: True
urxvt*secondaryScroll: True
!urxvt*background: #5b166b
urxvt*foreground: #ffffff
urxvt*urlLauncher: firefox

! Fake Transparency 
urxvt*depth: 32 
urxvt*background: rgba:0000/0000/0200/c800
urxvt*transparent: true
urxvt*shading: 25

! special
*.foreground:   #a39e9b
*.background:   #2f1e2e
*.cursorColor:  #a39e9b

! black
*.color0:       #2f1e2e
*.color8:       #776e71

! red
*.color1:       #ef6155
*.color9:       #ef6155

! green
*.color2:       #48b685
*.color10:      #48b685

! yellow
*.color3:       #fec418
*.color11:      #fec418

! blue
*.color4:       #06b6ef
*.color12:      #06b6ef

! magenta
*.color5:       #815ba4
*.color13:      #815ba4

! cyan
*.color6:       #5bc4bf
*.color14:      #5bc4bf

! white
*.color7:       #a39e9b
*.color15:      #e7e9db
